{
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "type-check": "vue-tsc --noEmit",
    "format-check": "prettier \"resources/**/*.{js,ts,vue}\" --check",
    "prettify": "prettier \"resources/**/*.{js,ts,vue}\" --write",
    "lint-check": "eslint \"resources/**\" ",
    "lint": "eslint \"resources/**\" --fix",
    "build": "vue-tsc && vite build",
    "check-build": "run-p type-check format-check lint-check && build"
  },
  "devDependencies": {
    "@eslint/js": "^9.6.0",
    "@inertiajs/vue3": "^1.0.0",
    "@tailwindcss/forms": "^0.5.3",
    "@vitejs/plugin-vue": "^5.0.0",
    "alpinejs": "^3.4.2",
    "autoprefixer": "^10.4.2",
    "axios": "^1.6.4",
    "eslint": "^9.6.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-vue": "^9.27.0",
    "globals": "^15.8.0",
    "laravel-vite-plugin": "^1.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.31",
    "prettier": "^3.3.2",
    "prettier-plugin-organize-attributes": "^1.0.0",
    "prettier-plugin-organize-imports": "^4.0.0",
    "prettier-plugin-tailwindcss": "^0.6.5",
    "tailwindcss": "^3.1.0",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.15.0",
    "vite": "^5.0",
    "vite-plugin-eslint": "^1.8.1",
    "vue": "^3.4.0",
    "vue-tsc": "^2.0.21"
  }
}
